{
  "name": "wikilinks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:full": "concurrently \"vite --host\" \"npx ts-node server/src/server.ts\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean-install": "rimraf node_modules package-lock.json .next && npm install",
    "clean-install:full": "rimraf node_modules package-lock.json .next server/node_modules server/package-lock.json server/.next && npm install && cd server && npm install"
  },
  "dependencies": {
    "@mantine/core": "^7.17.2",
    "@mantine/hooks": "^7.17.2",
    "@tabler/icons-react": "^3.31.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "html-react-parser": "^5.2.2",
    "mongoose": "^8.13.2",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use-websocket": "^4.13.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.3",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.2.0",
    "rimraf": "^6.0.1",
    "socket.io": "^4.8.1",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
